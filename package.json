{
  "name": "@nekofar/nouns",
  "version": "1.0.0-alpha.3",
  "private": false,
  "description": "TypeScript client for interacting with Nouns",
  "keywords": [
    "nouns"
  ],
  "homepage": "https://github.com/nekofar/nouns#readme",
  "bugs": {
    "url": "https://github.com/nekofar/nouns/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekofar/nouns.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Milad Nekofar",
    "email": "milad@nekofar.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./contracts": {
      "types": "./dist/contracts/index.d.ts",
      "import": "./dist/contracts/index.mjs",
      "require": "./dist/contracts/index.cjs"
    },
    "./subgraphs": {
      "types": "./dist/subgraphs/index.d.ts",
      "import": "./dist/subgraphs/index.mjs",
      "require": "./dist/subgraphs/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "pnpm clean && pnpm graphql:generate && pnpm wagmi:generate",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsup",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "graphql:generate": "graphql-codegen --config codegen.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "test": "echo 'No tests available.' && exit 0",
    "wagmi:generate": "wagmi generate"
  },
  "dependencies": {
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@graphprotocol/graph-ts": "^0.38.0",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/schema-ast": "^4.1.0",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@parcel/watcher": "^2.5.1",
    "@types/node": "^22.13.14",
    "@typescript-eslint/utils": "^8.28.0",
    "@wagmi/cli": "^2.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "globals": "^16.0.0",
    "graphql-scalars": "^1.24.2",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "rimraf": "^6.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "viem": "^2.27.2",
    "wagmi": "^2.14.16"
  },
  "packageManager": "pnpm@10.5.2",
  "publishConfig": {
    "access": "public"
  }
}
