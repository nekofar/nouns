{
  "name": "@nekofar/nouns",
  "version": "1.0.0-alpha.36",
  "private": false,
  "description": "TypeScript client for interacting with Nouns",
  "keywords": [
    "nouns"
  ],
  "homepage": "https://github.com/nekofar/nouns#readme",
  "bugs": {
    "url": "https://github.com/nekofar/nouns/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nekofar/nouns.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Milad Nekofar",
    "email": "milad@nekofar.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./contracts": {
      "require": "./dist/contracts/index.cjs",
      "import": "./dist/contracts/index.mjs"
    },
    "./subgraphs": {
      "require": "./dist/subgraphs/index.cjs",
      "import": "./dist/subgraphs/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist/**",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "is-ci && exit 0 || (pnpm clean && pnpm graphql:generate && pnpm wagmi:generate)",
    "build": "NODE_OPTIONS=\"--max-old-space-size=8192\" tsdown",
    "clean": "rimraf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "graphql:generate": "graphql-codegen --config codegen.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "wagmi:generate": "wagmi generate"
  },
  "resolutions": {
    "@metamask/sdk": ">=0.33.1",
    "@metamask/sdk-communication-layer": ">=0.33.1",
    "sha.js": ">= 2.4.12"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "^1.12.16",
    "@eslint/js": "^9.25.0",
    "@graphprotocol/graph-ts": "^0.38.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/schema-ast": "^5.0.0",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^5.0.0",
    "@parcel/watcher": "^2.5.1",
    "@types/node": "^25.0.0",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/utils": "^8.30.1",
    "@wagmi/cli": "^2.2.1",
    "dotenv": "^17.2.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsdoc": "^61.0.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "graphql-scalars": "^1.24.2",
    "is-ci": "^4.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.10",
    "rimraf": "^6.0.1",
    "tsdown": "^0.18.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^4.0.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.74.4",
    "@wagmi/core": "^2.16.7",
    "graphql": "^16.10.0",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "typescript": ">=5.0.0",
    "viem": "^2.27.2",
    "wagmi": "^2.14.16"
  },
  "packageManager": "pnpm@10.25.0",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": "^24",
    "pnpm": "^10"
  }
}
